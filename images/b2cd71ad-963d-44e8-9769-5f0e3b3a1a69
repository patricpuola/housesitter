				<script>
    var iFrame;
    function onLoad(element) {
        iFrame = element;
    }

    var prevHeight;
    function receiveMessage(event)
    {
        if (iFrame) {
            if (event.data != prevHeight) {
                iFrame.style.height = event.data + 'px';
                prevHeight = event.data;
                window.scrollTo(0, 0);
            }
        }
    }
    window.addEventListener("message", receiveMessage, false);

    </script>
