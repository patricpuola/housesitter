	<script>
		$(document).ready(function() {
			$(".asuntohakemus .pdf_btn").click(function(e){
				var field = $('#vuokrasuhdealkaa');
				var datevalue = field.val();

				if(datevalue === '' || datevalue === 'pp.kk.vvvv')
				{field.val('');}

				else if( ! datevalue.match(/^(0[1-9]|[12][0-9]|3[01])[\- \..](?:(0[1-9]|1[012])[\- \..](201)[2-9]{1})$/))
				{
					// Not correct date format
					e.preventDefault();

					var error_message = 'Ole hyvä ja tarkista päiväyksen muoto kentässä "Vuokrasuhde voi alkaa aikaisintaan" (pp.kk.vvvv) ';

					alert(error_message);
					return;
				}

				$('.asuntohakemus h1').hide();
				$("#asuntohakemuslomake,.intro").hide();
				$('.hidden-text').show();
				ga('aaTracker.send', 'pageview', 'tietopankit/asuntohakemus/kiitos');
				ga('aaTracker.send', 'event', 'button', 'click', 'tietopankit/asuntohakemus/kiitos');
				$("html, body").animate({ scrollTop: 0 }, "slow");
				//return false;
			});
		});
	</script>

