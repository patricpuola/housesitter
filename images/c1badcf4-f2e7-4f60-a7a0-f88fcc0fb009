				<script>
					// Functionality for opening the lang menu and closing it when clicking outside it
					(function() {
						var containerClass = 'footer-lang';
						var openClass = 'footer-lang--open';
						var buttonClass = 'footer-lang__toggle';
						var menuClass = 'footer-lang__menu';
						var container = document.querySelector('.' + containerClass);

						var onBodyClick = function(event) {
							var node = event.target;
							while (node && node.classList) {
								// Bail out if clicked inside the lang menu
								if (node.classList.contains(menuClass)) {
									return;
								}
								node = node.parentNode;
							}
							container.classList.remove(openClass);
							window.removeEventListener('click', onBodyClick);
						};

						document
							.querySelector('.' + buttonClass)
							.addEventListener('click', function(event) {
								if (!container.classList.contains(openClass)) {
									container.classList.add(openClass);
									window.addEventListener('click', onBodyClick);
									event.stopPropagation();
								}
							});
					}());
				</script>
