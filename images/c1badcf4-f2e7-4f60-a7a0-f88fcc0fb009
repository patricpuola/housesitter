let data;
try {
	data = window.localStorage.getItem('forenom.com/data');
} catch (e) {}

const user = data ? JSON.parse(data).user.data : null;
const type = 'aparthotel';
const skip1 = ['apartment','aparthotel','hostel'];
const skip2 = ['location-search','cashier'];

const uriCount = window.location.pathname.slice(1, -1).split("/").length;

