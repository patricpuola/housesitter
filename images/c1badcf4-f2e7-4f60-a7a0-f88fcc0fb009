				<script>
			(function(){
				function getIn(obj, path) {
					return path.reduce(function(memo, currentPath) {
						if (memo && memo.hasOwnProperty(currentPath)) {
							return memo[currentPath];
						}
						return null;
					}, obj);
				}

				var localData;
				try {
					localData = JSON.parse(localStorage.getItem('forenom.com/data'));
				} catch (e) {
					return;
				}

				var isLoggedIn = getIn(
					localData,
					['user', 'data', 'authentication', 'data', 'loggedIn']
				);
				var accountDetails = getIn(
					localData,
					['user', 'data', 'details', 'data', 'account', 'data']
				);
				var userLink = document.querySelector('.js-nav-username-inject');

				if (userLink && isLoggedIn && accountDetails) {
					userLink.innerText = accountDetails.firstName || 'Kirjautunut';
				}
			})()
